import random
class Animal:
    def __init__(self, species, name, age, health, hunger, happiness):
        super().__init__()
        self.apecies=species
        self.name=name
        self.age=age
        self.health=health
        self.hunger=hunger
        self.happines=happines
class Zoo:
    def __init__(self):
        super.__init__()
        self.animal=[]
